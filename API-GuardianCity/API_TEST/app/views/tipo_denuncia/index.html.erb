<p id="notice"><%= notice %></p>

<h1>Tipo Denuncia</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Nombre</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_denuncia.each do |tipo_denuncium| %>
      <tr>
        <td><%= tipo_denuncium.id %></td>
        <td><%= tipo_denuncium.nombre %></td>
        <td><%= tipo_denuncium.descripcion %></td>
        <td><%= link_to 'Show', tipo_denuncium %></td>
        <td><%= link_to 'Edit', edit_tipo_denuncium_path(tipo_denuncium) %></td>
        <td><%= link_to 'Destroy', tipo_denuncium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Denuncium', new_tipo_denuncium_path %>
